   "source": [
    "Given an integer num, repeatedly add all its digits until the result has only one digit, and return it."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "0"
      ]
     },
     "execution_count": 22,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "num = 0\n",
    "while num // 10:\n",
    "    temp = 0\n",
    "    while num // 10 or num % 10:\n",
    "        temp += num % 10\n",
    "        num //= 10\n",
    "    num = temp\n",
    "print(num)"
   ]
  }
